{
    "name": "ChatlogColors",
    "version": "1.0.0",
    "description": "It colors your chatlogs with colors based on name!",
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://pony.town/*",
                "http://pony.town/*"
            ],
            "js": [
                "chatlogcolors.js"
            ]
        }
    ]
}